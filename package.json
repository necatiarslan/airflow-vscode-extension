{
	"name": "airflow-vscode-extension",
	"displayName": "Airflow Vs Code Extension",
	"description": "An extention for VsCode to be able to manage Apache Airflow, run and troubleshot DAGs",
	"version": "0.0.1",
	"publisher": "Necati ARSLAN",
	"repository": {
		"type": "git",
		"url": "https://github.com/necatiarslan/airflow-vscode-extension"
	},
	"engines": {
		"vscode": "^1.69.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:airflow-vscode-extension.helloWorld",
		"onView:airflowView"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "airflowView.refreshServer",
				"title": "Refresh",
				"icon": {
					"light": "media/light/refresh.svg",
					"dark": "media/dark/refresh.svg"
				}
			},
			{
				"command": "airflowView.addServer",
				"title": "Add",
				"icon": {
					"light": "media/light/add.svg",
					"dark": "media/dark/add.svg"
				}
			},
			{
				"command": "airflowView.filter",
				"title": "Filter",
				"icon": {
					"light": "media/light/folder.svg",
					"dark": "media/dark/folder.svg"
				}
			},
			{
				"command": "airflowView.viewDagView",
				"title": "View"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "airflowView.refreshServer",
					"when": "view == airflowView",
					"group": "navigation"
				},
				{
					"command": "airflowView.addServer",
					"when": "view == airflowView",
					"group": "navigation"
				},
				{
					"command": "airflowView.filter",
					"when": "view == airflowView",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "airflowView.viewDagView",
					"when": "view == airflowView",
					"group": "inline"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "airflow-explorer",
					"title": "Airflow",
					"icon": "media/airflow.svg"
				}
			]
		},
		"views": {
			"airflow-explorer": [
				{
					"id": "airflowView",
					"name": "Airflow",
					"icon": "media/airflow.svg"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "airflowView",
				"contents": "[Add Airflow Server](command:airflowView.addServer)\n\n\n\n\n\n[learn more](https://github.com/necatiarslan/airflow-vscode-extension/blob/main/README.md)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/base-64": "^1.0.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.69.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"@vscode/test-electron": "^2.1.5",
		"eslint": "^8.18.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"ts-loader": "^9.3.1",
		"typescript": "^4.7.4",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0"
	},
	"dependencies": {
		"base-64": "^1.0.0",
		"node-fetch": "^3.2.9"
	}
}
